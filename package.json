		"pug-cli": "^1.0.0-alpha6"
	},
	"repository": {
		"url": "https://github.com/lukechu10/Minecraft-Box-Launcher",
		"type": "git"
	},
	"dependencies": {
		"@types/bytebuffer": "^5.0.40",
		"@types/fast-html-parser": "^1.0.0",
		"@types/got": "^9.6.7",
		"@types/yauzl": "^2.9.1",
		"@xmcl/minecraft-launcher-core": "^0.1.20",
		"electron-store": "^5.0.0",
		"jquery": "^3.4.1",
		"typescript": "^3.6.3"
	}
