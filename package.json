{
	"private": true,
	"name": "minecraft-box-launcher",
	"version": "0.0.1",
	"description": "Open source Minecraft Java Edition launcher build with ElectronJS",
	"main": "js/main.js",
	"scripts": {
		"start": "electron .",
		"start:gitpod": "electron --no-sandbox js/main.js",
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "run-p build:*",
		"build-watch:ts": "tsc -p tsconfig.json --watch",
		"build:ts": "tsc -p tsconfig.json",
		"build-watch:pug": "pug ./src/templates --watch --out ./static/views",
		"build:pug": "pug ./src/templates --out ./static/views",
		"eslint": "eslint src/ --ext .ts,.tsx",
		"pack": "electron-builder --dir",
		"dist": "electron-builder",
		"postinstall": "run-p build:*"
	},
	"keywords": [
		"minecraft",
		"launcher"
	],
	"author": "lukechu10",
	"license": " GPL-3.0-or-later",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^2.3.1",
		"@typescript-eslint/parser": "^2.3.1",
		"electron": "^6.0.10",
		"electron-builder": "^21.2.0",
		"eslint": "^6.5.0",
		"npm-run-all": "^4.1.5",
		"pug-cli": "^1.0.0-alpha6"
	},
	"repository": {
		"url": "https://github.com/lukechu10/Minecraft-Box-Launcher",
		"type": "git"
	},
	"dependencies": {
		"@types/bytebuffer": "^5.0.40",
		"@types/fast-html-parser": "^1.0.0",
		"@types/got": "^9.6.7",
		"@types/jquery": "^3.3.31",
		"@types/semantic-ui": "^2.2.7",
		"@types/yauzl": "^2.9.1",
		"@xmcl/minecraft-launcher-core": "^0.1.22",
		"electron-store": "^5.0.0",
		"jquery": "^3.4.1",
		"typescript": "^3.6.3"
	}
}